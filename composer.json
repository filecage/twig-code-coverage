{
    "name": "thomann/twig-code-coverage",
    "description": "A library for code coverage reports and Twig snapshot tests",
    "type": "library",
    "autoload":  {
        "psr-4": {
            "Thomann\\TwigCodeCoverage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thomann\\TwigCodeCoverageTests\\": "tests/"
        }
    },
    "require": {
        "ext-xdebug": "*",
        "php": "7.4.*",
        "twig/twig": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
